        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false"
            position="named">
          <maml:name>Topic</maml:name>
          <maml:description>
            <maml:para>Search for messages based on group chat name. </maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
      <command:syntaxItem>
        <maml:name>Search-TeamsMessage</maml:name>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false"
            position="named">
          <maml:name>ThreadId</maml:name>
          <maml:description>
            <maml:para>Search for messages based on thread Id.</maml:para>
