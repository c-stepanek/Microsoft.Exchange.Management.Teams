    <command:details>
      <command:name>Search-TeamsMessage</command:name>
      <maml:description>
        <maml:para>Searches for Teams messages within an Exchange mailbox.</maml:para>
      </maml:description>
      <maml:copyright>
        <maml:para />
      </maml:copyright>
      <command:verb>Search</command:verb>
      <command:noun>TeamsMessage</command:noun>
      <dev:version />
    </command:details>
    <maml:description>
      <maml:para>
        This cmdlet leverages the EWS Managed API to search for Teams messages within an Exchange mailbox.
      </maml:para>
    </maml:description>
    <command:syntax>
      <command:syntaxItem>
        <maml:name>Search-TeamsMessage</maml:name>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false"
            position="named">
          <maml:name>Sender</maml:name>
          <maml:description>
            <maml:para>The sender of the Teams chat message.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false"
            position="named">
          <maml:name>Recipient</maml:name>
          <maml:description>
            <maml:para>The recipient of the Teams chat message.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
        </command:parameter>
        <command:parameter required="true" variableLength="false" globbing="false" pipelineInput="false"
            position="named">
          <maml:name>MessageContains</maml:name>
          <maml:description>
            <maml:para>The message text you want to search for.</maml:para>
          </maml:description>
          <command:parameterValue required="true" variableLength="false">String</command:parameterValue>
        </command:parameter>
      </command:syntaxItem>
      <command:syntaxItem>
